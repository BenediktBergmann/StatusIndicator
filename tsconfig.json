{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",                    
    "lib": ["es6"],                     
    "allowJs": true,
    "outDir": "./dist/", 
    "strict": true,  
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
